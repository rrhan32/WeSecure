<body>
    <div class="parent">
        
        <a href="website1.html"><div class="child1"></a>
            <div class="rj" style="color:whitesmoke ">WatchAI <br>To protect your loved ones. </div>
            <div class="whyus" ><a href="about" style="color:whitesmoke">WHY US?</a></div>
        </div>
        <div class="child2">
            <form action="http://localhost:3000/livestream" class="shadow p-3 mb-5 bg-body "  >
                <div class="row">
                    <div class="col-12">
                    <label for="email"></label>
                    <input type="email"  placeholder="Enter Email" class="form-control" id="email">
                    </div>
                    <div class="col-12">
                        <label for="password" ></label>
                        <input type="password" placeholder="Password" class="form-control" id="password">
                    </div>
                    <div class="col-12">
                    <label for="number"></label>
                    <input type="phone-number"  placeholder="Enter Phone Number" class="form-control" id="number">
                    </div>
                    <div class="col-12">
                    <br><button class="btn btn-primary form-control" type="submit" href="http://localhost:3000/livestream">LOGIN</button>
                    </div>
                    <div class="col-12 text-center">
                    <br><a href="http://localhost:3000/forgot">Forgot Password?</a>
                    </div>
                    </div>
                    <div class="col-12">
                    <hr><br><button class="btn btn-success form-control" type="button" ><a href="http://localhost:3000/create_account" >Create New Account</a> </button>
                    </div>
                </div>      
            </form>
        </div>
    </div>
    <footer class="w3-center w3-dark-blue w3-padding-64">
        <a href="http://localhost:3000/" class="w3-button w3-light-grey"><i class="fa fa-arrow-up w3-margin-right"></i>Home Page</a>
        <div class="w3-xlarge w3-section">
          <i class="fa fa-facebook-official w3-hover-opacity"></i>
          <i class="fa fa-instagram w3-hover-opacity"></i>
          <i class="fa fa-snapchat w3-hover-opacity"></i>
          <i class="fa fa-pinterest-p w3-hover-opacity"></i>
          <i class="fa fa-twitter w3-hover-opacity"></i>
          <i class="fa fa-linkedin w3-hover-opacity"></i>
        </div>
        <p>Powered by <a href="http://localhost:3000/" title="WatchAI" target="_blank" class="w3-hover-text-green">WatchAI</a></p>
      </footer>
       
      <script>
      // Modal Image Gallery
      function onClick(element) {
        document.getElementById("img01").src = element.src;
        document.getElementById("modal01").style.display = "block";
        var captionText = document.getElementById("caption");
        captionText.innerHTML = element.alt;
      }
      
      
      // Toggle between showing and hiding the sidebar when clicking the menu icon
      var mySidebar = document.getElementById("mySidebar");
      
      function w3_open() {
        if (mySidebar.style.display === 'block') {
          mySidebar.style.display = 'none';
        } else {
          mySidebar.style.display = 'block';
        }
      }
      
      // Close the sidebar with the close button
      function w3_close() {
          mySidebar.style.display = "none";
      }
      </script>
    
    

    
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
